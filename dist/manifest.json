{
  "manifest_version": 2,
  "name": "its",
  "version": "0.2.0",
  "description": "Detect trailing spaces while you type in any text or search input",
  "icons": {
    "48": "assets/icons/its_icon_48.png"
  },
  "options_ui": {
    "page": "options.html"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "assets/js/app.js"
      ]
    }
  ]
}
